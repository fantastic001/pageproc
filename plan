Document 
	
	Document: string -> void 
	
	page: int -> Page 
	filename: void -> string 
	qfile: void -> QFile // USE QT

Page 
	
	process: Classificator & Segmentator 

Segmentator: 
	
	getParts: void -> vector<MatPart> 

Classificator: 
	
	recognizeTable: MatPart -> bool 
	recognizeText: MatPart -> bool 
	recognizeDrawing: MatPart -> bool 

Settings: 
	
	values: map<myunion> 

